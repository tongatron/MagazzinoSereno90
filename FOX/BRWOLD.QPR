SELECT ARKIVOLD.CODICE, ARKIVOLD.COD_PD, ARKIVOLD.DESCRIZ,;
  ARKIVOLD.COSTO, ARKIVOLD.ULTVEN, ARKIVOLD.GIACONT, ARKIVOLD.GIADIN,;
  ARKIVOLD.PROGCAR, ARKIVOLD.PROGSCA;
 FROM ARKIVOLD;
 WHERE ARKIVOLD.PRD = "ESC";
 ORDER BY ARKIVOLD.COD_PD;
 INTO CURSOR QUERY
BROWSE NOMODIFY PREFERENCE QUERY
