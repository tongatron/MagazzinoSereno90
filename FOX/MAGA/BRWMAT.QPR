SELECT ARKIVIO.COD_PD, ARKIVIO.DESCRIZ, ARKIVIO.COSTO, ARKIVIO.PREZZO_1,;
  ARKIVIO.GIACONT, ARKIVIO.PROGCAR, ARKIVIO.PROGSCA, ARKIVIO.GIADIN,;
  ARKIVIO.CODICE;
 FROM ARKIVIO;
 WHERE UPPER(ARKIVIO.PRD) = "PEG";
 ORDER BY ARKIVIO.COD_PD;
 INTO CURSOR QUERY
BROWSE NOMODIFY PREFERENCE QUERY

