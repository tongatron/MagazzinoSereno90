SELECT Arkivio.codice, Arkivio.cod_pd, Arkivio.descriz, Arkivio.costo,;
  Arkivio.prezzo_1, Arkivio.ultven, Arkivio.giadin, Arkivio.ultcar,;
  Arkivio.progcar, Arkivio.progsca, Arkivio.carvalor, PREZZO_1/COSTO,;
  Arkivio.iva, Arkivio.cbarre;
 FROM Arkivio;
 WHERE Arkivio.prd = "SON";
   AND Arkivio.progcar > 0
