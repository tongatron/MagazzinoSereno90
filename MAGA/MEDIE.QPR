SELECT ARKIVIO.CODICE, ARKIVIO.PRD, ARKIVIO.COD_PD, ARKIVIO.DESCRIZ,;
  ARKIVIO.COSTO, ARKIVIO.PREZZO_1, ARKIVIO.GIACONT, ARKIVIO.GIADIN,;
  ARKIVIO.PROGCAR, ARKIVIO.PROGSCA, ARKIVIO.PREZZO_1 / ARKIVIO.COSTO;
 FROM ARKIVIO;
 WHERE ARKIVIO.COSTO > 1;
   AND ARKIVIO.PREZZO_1 > 1;
 INTO CURSOR QUERY
BROWSE NOMODIFY PREFERENCE QUERY
