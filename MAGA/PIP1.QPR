SELECT ARKIVIO.CODICE, ARKIVIO.COD_PD, ARKIVIO.DESCRIZ, ARKIVIO.COSTO,;
  ARKIVIO.PREZZO_1, ARKIVIO.GIACONT, ARKIVIO.PROGCAR, ARKIVIO.PROGSCA,;
  ARKIVIO.GIADIN, ARKIVIO.ULTCAR, ARKIVIO.ULTVEN;
 FROM ARKIVIO;
 WHERE ARKIVIO.CODICE = "VAR1";
 ORDER BY ARKIVIO.COD_PD;
 INTO CURSOR QUERY
BROWSE NOMODIFY
