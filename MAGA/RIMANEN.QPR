SELECT ARKIVIO.CODICE, ARKIVIO.PRD, ARKIVIO.DESCRIZ, ARKIVIO.COSTO,;
  ARKIVIO.GIACONT, ARKIVIO.PROGCAR, ARKIVIO.PROGSCA, ARKIVIO.GIADIN,;
  ARKIVIO.ULTCAR;
 FROM ARKIVIO;
 WHERE ARKIVIO.GIACONT > 1;
   AND ARKIVIO.PROGSCA > 1;
   AND ARKIVIO.COSTO > 15000;
 ORDER BY ARKIVIO.PRD;
 INTO CURSOR QUERY
BROWSE NOMODIFY PREFERENCE QUERY
