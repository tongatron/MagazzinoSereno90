SELECT ARKIVOLD.CODICE, ARKIVOLD.PRD, ARKIVOLD.COD_PD, ARKIVOLD.DESCRIZ,;
  ARKIVOLD.COSTO, ARKIVOLD.ULTVEN, ARKIVOLD.COST_MED, ARKIVOLD.COST_CONT,;
  ARKIVOLD.GIACONT, ARKIVOLD.GIADIN, ARKIVOLD.ULTCAR, ARKIVOLD.PROGCAR,;
  ARKIVOLD.PROGSCA, ARKIVOLD.CARVALOR;
 FROM ARKIVOLD;
 WHERE ARKIVOLD.CAT = "ELE";
   AND ARKIVOLD.GIADIN > 0;
   AND ARKIVOLD.PROGCAR > 0;
 ORDER BY ARKIVOLD.PRD, ARKIVOLD.COD_PD;
 INTO CURSOR QUERY
BROWSE NOMODIFY PREFERENCE QUERY

